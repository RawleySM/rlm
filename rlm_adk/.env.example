# RLM-ADK Environment Configuration
# Copy this file to .env and fill in your values

# =============================================================================
# Google AI / Vertex AI Configuration
# =============================================================================

# For Google AI Studio (simpler setup, good for development)
GOOGLE_GENAI_USE_VERTEXAI=0
GOOGLE_API_KEY=your-google-api-key-here

# For Google Cloud Vertex AI (production)
# GOOGLE_GENAI_USE_VERTEXAI=1
# GOOGLE_CLOUD_PROJECT=your-gcp-project-id
# GOOGLE_CLOUD_LOCATION=us-east1

# =============================================================================
# Databricks Configuration
# =============================================================================

# Databricks workspace URL (without https://)
DATABRICKS_HOST=your-workspace.cloud.databricks.com

# Personal access token for Databricks
DATABRICKS_TOKEN=dapi-your-token-here

# Cluster ID for Python REPL execution
DATABRICKS_CLUSTER_ID=your-cluster-id

# SQL Warehouse ID for SQL query execution
DATABRICKS_SQL_WAREHOUSE_ID=your-warehouse-id

# =============================================================================
# Unity Catalog Configuration (Optional)
# =============================================================================

# Default catalog for operations
DEFAULT_CATALOG=healthcare_main

# Masterdata catalog name
MASTERDATA_CATALOG=masterdata_vendors

# Hospital chain catalogs (comma-separated)
HOSPITAL_CHAIN_CATALOGS=hospital_chain_alpha,hospital_chain_beta,hospital_chain_gamma
